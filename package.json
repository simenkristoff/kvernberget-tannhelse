{
  "name": "kvernberget-tannhelse",
  "private": true,
  "version": "1.0.0",
  "description": "",
  "main": "package.json",
  "author": "Simen Kristoffersen <simekri@stud.ntnu.no>",
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "https://github.com/simenkristoff/kvernberget-tannhelse.git"
  },
  "workspaces": {
    "nohoist": [
      "client/**",
      "studio/**"
    ],
    "packages": [
      "apps/*",
      "packages/*"
    ]
  },
  "scripts": {
    "postinstall": "yarn husky install",
    "build": "turbo run build",
    "dev": "turbo run dev --parallel --no-cache",
    "test": "turbo run test",
    "lint": "turbo run lint",
    "codegen": "turbo run codegen",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,md}\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && yarn lint"
    }
  },
  "devDependencies": {
    "@types/lodash": "^4.14.178",
    "@types/node": "^17.0.18",
    "@types/react": "^17.0.39",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.3",
    "prettier": "^2.5.1",
    "pretty-quick": "^3.1.3",
    "sanity-codegen": "^0.9.8",
    "turbo": "latest",
    "typescript": "^4.5.5"
  },
  "packageManager": "yarn@1.22.17",
  "dependencies": {
    "@typescript-eslint/parser": "^5.12.0",
    "lodash": "^4.17.21"
  }
}